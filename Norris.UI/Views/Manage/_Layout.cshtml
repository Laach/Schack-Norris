@{
    Layout = "/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Identity
@using Norris.UI.Models
@using Norris.Data.Data.Entities

    <div class="row" id="siteGrid">
        <div class="col-md-1"></div>
        <div class="col-md-3">
            <div class="panel panel-default" id="panel-override">
                <div class="panel-heading" id="panel-heading-override"><span id="title">Menu</span></div>

                @await Html.PartialAsync("_ManageNav")
            </div>
        </div>
        <div class="col-md-6">
            @RenderBody()
        </div>
        <div class="col-md-1"></div>

    </div>
    <h2>Profile</h2>
    @inject UserManager<User> UserManager



    @section Scripts {
        @RenderSection("Scripts", required: false)
    }

